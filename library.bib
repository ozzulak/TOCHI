@book{Coulter1989,
abstract = {This book explores recent developments in the sociology of knowledge, and highlights the shift away from traditional—particularly Cartesian—conceptions of person, mind and social behavior. Coulter argues that a new "epistemic" sociology has emerged in which the central focus is the social construction of the "Intelligibility" of phenomena, in everyday practical affairs as well as within the conduct of scientific inquiry. This approach is documented with lucid examples, and is shown to make possible a radical re-thinking of the "cognizing subject." The operations and accomplishments of "mind" (believing, understanding, thinking, etc.) are now conceived of as exhibited in action and interaction and amenable to socio-logical analysis. A variety of conceptions of mind and experience, such as the symbolic-interactionist and cognitive-computationalist perspectives, are considered, and some contemporary claims about the human personality, hypnotic states, the relationship of mind to brain and the relationship of reasons to actions are discussed. Aimed at an audience of advanced undergraduate and graduate students, this clear and informative overview also features developments of the author's previous contributions to the field. Interdisciplinary in scope, it is a thought-provoking and challenging addition to courses in contemporary sociological theory, social psychology, cognitive science and philosophy of the social sciences.},
author = {Coulter, Jeff},
publisher = {Humanities Press International},
title = {{Mind in action.}},
year = {1989}
}
