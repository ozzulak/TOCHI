journal = {Academic medicine},
keywords = {Clinical Clerkship,Clinical Clerkship: methods,Clinical Competence,Communication,Curriculum,Educational Measurement,Female,Humans,Male,Medical,Physician-Patient Relations,Schools,Sensitivity and Specificity,United States,intervention,key-paper,medical,medical students,political argument,socrev},
mendeley-tags = {intervention,key-paper,medical,medical students,political argument,socrev},
