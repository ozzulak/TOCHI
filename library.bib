journal = {Behavioral and cognitive neuroscience reviews},
keywords = {Adaptation,Adolescent,Adult,Awareness,Awareness: physiology,Cerebral Cortex,Cerebral Cortex: physiology,Child,Emotions,Emotions: physiology,Empathy,Humans,Infant,Models,Neurological,Perception,Perception: physiology,Preschool,Psychological,Self Concept,Social Behavior,empathy,key-paper,priority,review},
