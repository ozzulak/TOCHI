month = jul,
number = {3},
pages = {299--313},
title = {{Educational effects of the Tools of the Mind curriculum: A randomized trial}},
url = {http://dx.doi.org/10.1016/j.ecresq.2008.03.001},
volume = {23},
year = {2008}
}
