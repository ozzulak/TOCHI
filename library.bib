pages = {102--16},
pmid = {17223072},
title = {{Perception of arousal in social anxiety: effects of false feedback during a social interaction.}},
url = {http://dx.doi.org/10.1016/j.jbtep.2006.11.003},
volume = {39},
year = {2008}
}
