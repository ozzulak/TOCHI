pages = {401--421},
title = {{The contribution of the Responsive Classroom Approach on children's academic achievement: Results from a three year longitudinal study}},
url = {http://dx.doi.org/10.1016/j.jsp.2006.10.003},
volume = {45},
year = {2007}
}
