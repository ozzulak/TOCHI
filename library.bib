keywords = {Biofeedback, Psychology,Biofeedback, Psychology: methods,Heart Rate,Heart Rate: physiology,Humans},
month = sep,
number = {3},
pages = {229--42},
pmid = {20443135},
title = {{Biofeedback of heart rate variability and related physiology: a critical review.}},
