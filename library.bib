journal = {Prevention science : the official journal of the Society for Prevention Research},
keywords = {Child,Child Behavior Disorders,Child Behavior Disorders: prevention \& control,Child Behavior Disorders: psychology,Cognition,Female,Humans,Male,Models,Outcome Assessment (Health Care),Psychological,Regression Analysis,School Health Services,School Health Services: organization \& administrat,Social Adjustment,Verbal Behavior,Vocabulary,Washington,children,intervention,socrev},
