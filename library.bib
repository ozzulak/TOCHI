keywords = {Adult,Affect,Female,Humans,Interpersonal Relations,Male,Marriage,Marriage: psychology,Middle Aged,Personal Satisfaction,Social Support,Videotape Recording,Young Adult,couples,empathic accuracy,empathy},
mendeley-tags = {couples,empathic accuracy,empathy},
month = dec,
number = {6},
pages = {792--802},
pmid = {19102590},
title = {{Support provision in marriage: the role of emotional similarity and empathic accuracy.}},
