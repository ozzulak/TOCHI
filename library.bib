@article{Janssen2010,
abstract = {Despite a variety of new communication technologies, loneliness is prevalent in Western countries. Boosting emotional communication through intimate connections has the potential to reduce loneliness. New technologies might exploit biosignals as intimate emotional cues because of their strong relationship to emotions. Through two studies, we investigate the possibilities of heartbeat communication as an intimate cue. In the first study (\{$\backslash$rm N\} = 32), we demonstrate, using self-report and behavioral tracking in an immersive virtual environment, that heartbeat perception influences social behavior in a similar manner as traditional intimate signals such as gaze and interpersonal distance. In the second study (\{$\backslash$rm N\} = 34), we demonstrate that a sound of the heartbeat is not sufficient to cause the effect; the stimulus must be attributed to the conversational partner in order to have influence. Together, these results show that heartbeat communication is a promising way to increase intimacy. Implications and possibilities for applications are discussed.},
annote = {Good loneliness lit/effects  overview ... could use that if we decide to do something about loneliness. Seems like a good lit review also on other areas of intimate communication.
        
Based on a social psychology study from the 60ties -- looking at the interplay between distance/eye-contact/intimacy ... this study added sound of heartbeat. 
        
The participants we asked to come as close to a confederate as they feel comfortable and gaze into his eyes for 30 seconds. The conditions were (i) sound of heartbeat, supposedly real-time feedback from the confederate; (ii) sound of heartbeat, but taken from the internet (no connection to confederate); (iii) no sound. 
        
Results show larger distance for the "real" heartbeat only, the "internet heartbeat" had no effect. 
        
The studies are done in "Immersive Virtual Environment", used to control for confederate's behaviour, also allows reliable measurements ... but takes away reality (what about validity?)
        
Very stringent social psychology experiment ... could be used as a good argument to see what happens in practice? 
        
Only a one-sided study again (i.e., the participants did not hear their heartbeat at the same time)},
author = {Janssen, Joris H. and Bailenson, Jeremy N. and IJsselsteijn, Wijnand A. and Westerink, Joyce H.D.M.},
doi = {10.1109/T-AFFC.2010.13},
file = {:Users/ozzik/Library/Application Support/Mendeley Desktop/Downloaded/Janssen et al. - 2010 - Intimate Heartbeats Opportunities for Affective Communication Technology.pdf:pdf},
issn = {1949-3045},
journal = {IEEE Transactions on Affective Computing},
keywords = {BTD,BTD-group,HCI,heartbeat,key-paper,loneliness,psychology},
mendeley-tags = {BTD,BTD-group,HCI,heartbeat,key-paper,loneliness,psychology},
month = jul,
number = {2},
pages = {72--80},
publisher = {Published by the IEEE Computer Society},
title = {{Intimate Heartbeats: Opportunities for Affective Communication Technology}},
url = {http://www.computer.org/portal/web/csdl/doi/10.1109/T-AFFC.2010.13},
volume = {1},
year = {2010}
}
