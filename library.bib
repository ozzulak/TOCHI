keywords = {Adult,Family Therapy,Family Therapy: methods,Hallucinations,Hallucinations: therapy,Health Status,Humans,Male,Medication Adherence,Patient Satisfaction,Quality of Life,Schizophrenia,Schizophrenia: therapy,Treatment Outcome,Videoconferencing,telepsychiatry},
mendeley-tags = {telepsychiatry},
