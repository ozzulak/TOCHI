issn = {10715819},
journal = {International Journal of Human-Computer Studies},
keywords = {SenseCam,passive image capture,reflection,reflective practice,sensecam,teacher training},
mendeley-tags = {SenseCam},
month = dec,
number = {12},
pages = {1024--1036},
title = {{Teachers’ and tutors’ social reflection around SenseCam images}},
url = {http://dx.doi.org/10.1016/j.ijhcs.2009.09.004},
volume = {67},
year = {2009}
}
