journal = {Journal of personality and social psychology},
keywords = {Achievement,Adolescent,Defense Mechanisms,Dependency (Psychology),Female,Group Processes,Helping Behavior,Hierarchy, Social,Humans,Judgment,Male,Personal Autonomy,Self Concept,Social Change,Social Environment,Social Identification,group identity},
