keywords = {Affect,Affect: physiology,Algorithms,Artificial Intelligence,Automated,Automated: methods,CHIDC,Emotions,Emotions: physiology,Facial Expression,Monitoring,Pattern Recognition,Physiologic,Physiologic: methods,Sound Spectrography,Sound Spectrography: methods},
mendeley-tags = {CHIDC},
