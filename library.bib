@article{Batliner2003,
abstract = {Automatic dialogue systems used, for instance, in call centers, should be able to determine in a critical phase of the dialogue––indicated by the customers vocal expression of anger/irritation––when it is better to pass over to a human operator. At a first glance, this does not seem to be a complicated task: It is reported in the literature that emotions can be told apart quite reliably on the basis of prosodic features. However, these results are achieved most of the time in a laboratory setting, with experienced speakers (actors), and with elicited, controlled speech. We compare classification results obtained with the same feature set for elicited speech and for a Wizard-of-Oz scenario, where users believe that they are really communicating with an automatic dialogue system. It turns out that the closer we get to a realistic scenario, the less reliable is prosody as an indicator of the speakers’ emotional state. As a consequence, we propose to change the target such that we cease looking for traces of particular emotions in the users’ speech, but instead look for indicators of trouble in communication. For this reason, we propose the module Monitoring of User State [especially of] Emotion (mouse) in which a prosodic classifier is combined with other knowledge sources, such as conversationally peculiar linguistic behavior, for example, the use of repetitions. For this module, preliminary experimental results are reported showing a more adequate modelling of trouble in communication.},
author = {Batliner, A. and Fischer, K. and Huber, R. and Spilker, J. and N\"{o}th, E.},
doi = {10.1016/S0167-6393(02)00079-1},
issn = {01676393},
journal = {Speech Communication},
keywords = {annotation,automatic classification,dialogue,emotion,neural networks,prosody,review,spontaneous speech},
mendeley-tags = {review},
month = apr,
number = {1-2},
pages = {117--143},
title = {{How to find trouble in communication}},
url = {http://dx.doi.org/10.1016/S0167-6393(02)00079-1},
volume = {40},
year = {2003}
}
