month = jul,
number = {4},
pages = {289--298},
pmid = {20628581},
title = {{Response to a Relational Agent by Hospital Patients with Depressive Symptoms.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2901553\&tool=pmcentrez\&rendertype=abstract},
