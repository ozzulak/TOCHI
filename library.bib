journal = {Journal of general internal medicine},
keywords = {Communication,Education,Empathy,Humans,Medical,Medical: psychology,Physician-Patient Relations,Self Care,Students,Teaching,Teaching: methods,Undergraduate,Undergraduate: methods,empathy,key-paper,medical,medical students,review,socrev,teaching empathy},
