journal = {Education for health (Abingdon, England)},
keywords = {Adult,Curriculum,Education,Educational Measurement,Empathy,Female,Humans,Male,Medical,Nonparametric,Poetry as Topic,Program Evaluation,Statistics,Teaching,Teaching: methods,Undergraduate,Undergraduate: methods,empathy,priority,teaching empathy},
