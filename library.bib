month = feb,
number = {3},
pages = {765--769},
title = {{Can Communication Skills Training Alter Physicians' Beliefs and Behavior in Clinics?}},
url = {http://jco.ascopubs.org/cgi/content/abstract/20/3/765},
volume = {20},
year = {2002}
}
