number = {1},
pages = {26--55},
pmid = {20414477},
publisher = {Routledge},
title = {{Impact of the Positive Action program on school-level indicators of academic achievement, absenteeism, and disciplinary outcomes: A matched-pair, cluster randomized, controlled trial.}},
