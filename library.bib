issn = {1357-633X},
journal = {Journal of telemedicine and telecare},
keywords = {Adolescent,Attitude of Health Personnel,Child,Consumer Satisfaction,Humans,Patient Satisfaction,Professional-Patient Relations,Psychiatry,Psychiatry: economics,Psychiatry: methods,Randomized Controlled Trials as Topic,Telemedicine,Telemedicine: economics,Telemedicine: methods,telepsychiatry},
mendeley-tags = {telepsychiatry},
