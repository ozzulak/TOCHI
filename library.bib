issn = {1932-6203},
journal = {PloS one},
keywords = {Anatomy and physiology,Applied psychology,Behavior,Biology,Emotions,Engineering,Human factors engineering,Man computer interface,Medicine,Mental Health,Military psychology,Physiological processes,Physiology,Psychological stress,Psychology,Research Article,Social and behavioral sciences,Virtual reality,biofeedback,immersion,nottingham},
