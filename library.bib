series = {CHI '11},
title = {{Shared substance: developing flexible multi-surface applications}},
url = {http://doi.acm.org/10.1145/1978942.1979446},
year = {2011}
}
@inproceedings{Yamashita2011,
abstract = {In this study, we examine how remote collaborators' upper body view affects collaboration when people engage in multiparty fluid tabletop activities across distances. We experimentally investigated the effects of upper body view on four-person group tabletop collaboration, two-by-two at identical locations: shared tabletop vs. shared tabletop plus upper body view. Although previous research has often failed to illustrate the advantages of showing remote participants' upper body view, our study showed that task performance was significantly higher in conditions with upper body view. Furthermore, participants with upper body view tended to take a step away from their remote partners to effectively glance at them while taking a comparable perspective of the tabletop objects. Detailed analysis of the video recordings revealed that upper body view was effective for fluid tabletop collaboration because it helped achieve joint perspective and helped estimate the timing and rough location of subsequent tabletop activity.},
address = {New York, New York, USA},
annote = {Adding video images of remote partners upper body when collaborating over distance with tabletop. Same environment (t-room) as the "Hands on hitchcock: embodied reference to a moving scene" paper.
        
 Using "worker\&remote helper scenario" (a common task in some of the videoconferencing literature -- points to e.g., situations like remote doctor telling a less skilled coleague what to do etc). 
        
Good references to prior work on this topic. 
        
Argue that upper body video (vs. just images of hands on the surface) is important if the seating arrangement is to be kep flexible. 
        
They also try to analyse whether the users will prefer the "sit in each other's lap" (same orientation of artefacts) or a more distributed arrangement aroung the table, when given the freedom to choose. },
author = {Yamashita, Naomi and Kuzuoka, Hideaki and Hirata, Keiji and Aoyagi, Shigemi and Shirai, Yoshinari},
booktitle = {CHI '11},
doi = {10.1145/1978942.1979362},
file = {::},
isbn = {9781450302289},
keywords = {group-to-group collaboration,multitouch,priority-queue,remote gesture,shared tabletop,sitola-review,tabletop,upper body view,video-mediated communication},
mendeley-tags = {multitouch,priority-queue,sitola-review,tabletop},
month = may,
pages = {2827},
publisher = {ACM Press},
title = {{Supporting fluid tabletop collaboration across distances}},
url = {http://portal.acm.org/citation.cfm?id=1978942.1979362},
year = {2011}
}
@inproceedings{Bi:2011:MDB:1978942.1979309,
abstract = {Despite the prominence of multi-touch technologies, there has been little work investigating its integration into the desktop environment. Bringing multi-touch into desktop computing would give users an additional input channel to leverage, enriching the current interaction paradigm domi- nated by a mouse and keyboard. We provide two main con- tributions in this domain. First, we describe the results from a study we performed, which systematically evaluates the various potential regions within the traditional desktop con- figuration that could become multi-touch enabled. The study sheds light on good or bad regions for multi-touch, and also the type of input most appropriate for each of these regions. Second, guided by the results from our study, we explore the design space of multi-touch-integrated desk- top experiences. A set of new interaction techniques are coherently integrated into a desktop prototype, called Mag- ic Desk, demonstrating potential uses for multi-touch enabled desktop configurations.},
address = {New York, New York, USA},
annote = {Ozzik:
Interesting exploration of how multitouch surfaces can be included in a desktop workplace. 
        
Compare different regions around keyboard and mouse (plus the vertical touch display, that gives the worst results :). 
        
From autodesk research, with academy partners.  Could be relevant to our tabletop interactions (argues for the inclusion of keyboard/mouse if normal work is to be done). 
        
Vitek:
Two areas of interest:
1. investigation of single and multi-touch input abilities on the potential touch regions.
2. Exploration of designing the space on the desk and its integration with desktop environment (enhanced task-bar or multi-functional touch pad)
        
References covers ergonomic studies of physical and digital tabletop and touch-enhanced devices and environments
        
Small size experiment (10 people) made of three tasks:
1. single touch - strike
2. multi touch - docking the square by one hand
3. multi touch - docking the quare by two hands
        
Magic Desk
- based on MS Surface and 22" MT Dell display},
author = {Bi, Xiaojun and Grossman, Tovi and Matejka, Justin and Fitzmaurice, George},
booktitle = {CHI '11},
doi = {10.1145/1978942.1979309},
file = {::},
isbn = {9781450302289},
keywords = {desktop work,multi-touch,multitouch,priority-queue,sitola-review,tabletop},
mendeley-tags = {multitouch,priority-queue,sitola-review},
pages = {10},
publisher = {ACM Press},
series = {CHI '11},
title = {{Magic desk: bringing multi-touch surfaces into desktop work}},
url = {http://doi.acm.org/10.1145/1978942.1979309},
year = {2011}
}
