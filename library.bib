journal = {American journal of community psychology},
keywords = {Child,Educational Status,Female,Humans,Intervention Studies,Male,Observer Variation,Program Evaluation,Raising Healthy Children,SELSelect,Schools,Schools: statistics \& numerical data,Staff Development,Teaching,Teaching: methods,Washington,intervention,socrev},
