@article{VanDooren2012,
abstract = {Skin conductance (SC) is one of the most commonly used measures in psychophysiological studies involving emotional arousal and is traditionally measured at the fingers or the palms (i.e., the palmar locations) of the hand. Palmar skin conductance recording positions are, however, not always preferred for ambulatory recordings in real-life situations. This study quantifies the responsiveness and similarity with the finger of 16 different recording positions of skin conductance while watching emotional film fragments. Findings indicated foot, fingers and shoulders being most responsive, whereas arm, back, armpit, and thighbone were least responsive. The measurements at the foot were most similar with those of the finger. In contrast, arm, back, and armpit traces differed most from the finger trace. Taken together, foot and shoulders are the best alternatives to the finger for ambulatory measurement of skin conductance to reflect emotional arousal. These findings can help new applications using skin conductance, like automated emotion measurements, to come to fruition.},
author = {van Dooren, Marieke and de Vries, J J G Gert-Jan and Janssen, Joris H},
file = {:Users/ozzik/PhD/Test-literature/Emotional sweating across the body- Comparing 16 different skin conductance measurement locations.pdf:pdf},
issn = {1873-507X},
journal = {Physiology \& behavior},
keywords = {Adult,CHI13,Emotions,Emotions: physiology,Extremities,Extremities: physiology,Female,Fingers,Fingers: physiology,Galvanic Skin Response,Galvanic Skin Response: physiology,Head,Head: physiology,Humans,Male,Neck,Neck: physiology,Photic Stimulation,Photic Stimulation: methods,Psychophysiology,Psychophysiology: methods,Sweating,Sweating: physiology,Torso,Torso: physiology,Visual Perception,Visual Perception: physiology,psychophysiology,skin conductance},
mendeley-tags = {CHI13,psychophysiology,skin conductance},
month = may,
number = {2},
pages = {298--304},
title = {{Emotional sweating across the body: comparing 16 different skin conductance measurement locations.}},
url = {http://dx.doi.org/10.1016/j.physbeh.2012.01.020},
volume = {106},
year = {2012}
}
