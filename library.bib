month = oct,
number = {5},
pages = {711--31},
pmid = {10676545},
title = {{Opening the black box: using process evaluation measures to assess implementation and theory building.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10676545},
volume = {27},
year = {1999}
}
