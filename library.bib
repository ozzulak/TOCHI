@article{Robinson1982,
abstract = {Within the literature there is a claim that when 2 people develop an empathic relationship, there is a matching of their affective responses. Since emotional responses are often accompanied by physiological changes, it was hypothesized that some degree of physiological response correlation should be observable during the development of an empathic relationship. Finger skin temperature and skin conductance were monitored in 21 counselor–client pairs during counseling interviews. The Relationship Inventory Empathic Understanding subscale was used to measure the degree of empathy established during the interviews. Several methods were developed to quantify the degree of correspondence between counselor and client physiological responses. Results showed that matching of phasic responding was correlated with empathy, whereas matching of tonic physiological levels was not.},
author = {Robinson, John W. and Herman, Al and Kaplan, Bonnie J.},
