journal = {Medical education},
keywords = {Clinical Competence,Clinical Competence: standards,Communication,Curriculum,Education,Medical,Medical History Taking,Physician-Patient Relations,Politics,Sociology,Teaching,Teaching: methods,Undergraduate,Undergraduate: methods,medical,political argument,review,socrev},
mendeley-tags = {medical,political argument,review,socrev},
month = mar,
number = {3},
pages = {271--8},
pmid = {18275414},
