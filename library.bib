issn = {1747-0927},
journal = {Social neuroscience},
keywords = {Adult,Brain,Brain Mapping,Brain Mapping: methods,Brain: physiology,Cognition,Cognition: physiology,Emotions,Emotions: physiology,Empathy,Female,Humans,Imagination,Imagination: physiology,Male,Positron-Emission Tomography,Positron-Emission Tomography: methods,empathy,neuroscience},
mendeley-tags = {empathy,neuroscience},
month = jan,
