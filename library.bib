issn = {0002-8312},
journal = {American Educational Research Journal},
keywords = {HighScope,SELSelect,intervention,socrev},
mendeley-tags = {HighScope,SELSelect,intervention,socrev},
month = jan,
number = {3},
pages = {445--464},
title = {{Preschool Intervention, School Success and Delinquency in a High-Risk Sample of Youth}},
url = {http://aer.sagepub.com/content/22/3/445.short},
volume = {22},
year = {1985}
