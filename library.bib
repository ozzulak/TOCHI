keywords = {Brain,Brain: anatomy \& histology,Brain: physiology,Cerebral,Cerebral: physiology,Dominance,Emotions,Emotions: physiology,Empathy,Gyrus Cinguli,Gyrus Cinguli: anatomy \& histology,Gyrus Cinguli: physiology,Humans,Interpersonal Relations,Magnetic Resonance Imaging,appraisal,empathy,neuroscience},
mendeley-tags = {appraisal,empathy,neuroscience},
month = apr,
number = {2},
