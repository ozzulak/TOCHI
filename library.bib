issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Adult,Behavior,Female,Humans,Male,Middle Aged,Neurons,Neurons: physiology,Neurophysiology,Trust,Trust: psychology,neuroscience,trust},
mendeley-tags = {neuroscience,trust},
month = dec,
number = {50},
pages = {20084--9},
pmid = {18056800},
title = {{Neural correlates of trust.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2148426\&tool=pmcentrez\&rendertype=abstract},
