month = apr,
number = {16},
pages = {2101--2105},
title = {{Medical Interviewing and Interpersonal Skills Teaching in US Medical Schools: Progress, Problems, and Promise}},
url = {http://jama.ama-assn.org/cgi/content/abstract/269/16/2101},
volume = {269},
year = {1993}
